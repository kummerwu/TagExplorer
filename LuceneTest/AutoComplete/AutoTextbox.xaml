<UserControl x:Class="TagExplorer.AutoComplete.AutoTextbox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TagExplorer.AutoComplete"
             >
    <Canvas PreviewKeyDown="TextBox_PreviewKeyDown">
        <ComboBox Name="comboBox" Width="100" ></ComboBox>
        <TextBox Name="textBox"  Width="100"></TextBox>
    </Canvas>
</UserControl>
