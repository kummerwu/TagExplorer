<Window x:Class="TagExplorer.SearchResultListBox.InputBoxWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TagExplorer.SearchResultListBox"
        mc:Ignorable="d"
        Title="重命名" Width="300" Height="130" ShowInTaskbar="False" WindowStartupLocation="CenterOwner">
    <StackPanel>
        <Label Name="Tips" Height="30">hello world</Label>
        <TextBox Name="Input" Height="30"></TextBox>
        <StackPanel Height="30" Orientation="Horizontal">
            <Button Name ="OKBt" Width="60" Height="25" IsDefault="True" Margin="70,0,40,0" Click="OKBt_Click">确定</Button>
            <Button Name ="CancelBt" Width="60" Height="25" IsCancel="True" Click="CancelBt_Click">取消</Button>
        </StackPanel>
    </StackPanel>
</Window>
